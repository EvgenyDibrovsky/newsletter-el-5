        </mj-column>
      </mj-section>

      <mj-section padding-top="30px" padding-bottom="30px" padding-left="4%" padding-right="4%" background-color="#ffffff">
        <mj-column padding="0">
          <mj-image src="./images/icon-1.png" width="120px" height="120px" />
          <mj-text padding="0" font-weight="700" align="center" font-size="16px" color="#004580" font-family="Arial, Helvetica, sans-serif" line-height="22px">
